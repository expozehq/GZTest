          "_name": "form3",
          "_sourceTheme": "commercem4",
          "_customHTML": "<section group=\"Forms &amp; Maps\" class=\"form3\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"formstyler, datepicker\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" value=\"6\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" value=\"6\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n\n        <input type=\"color\" title=\"Labels Color\" name=\"labelsColor\" value=\"#151c15\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/cabbage-sprouts-isolated-171081-876-626x417.jpg\" parallax=\"\" selected=\"\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    <!-- End block parameters -->\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row justify-content-center\">\n            <div class=\"form-1 col-md-10 col-lg-8\">\n                <h2 mbr-theme-style=\"display-2\" class=\"mbr-fonts-style mbr-fonts-style mbr-section-title align-center\">\n                    Форма замовлення</h2>\n                <h3 mbr-theme-style=\"display-5\" class=\"mbr-fonts-style mbr-fonts-style mbr-section-subtitle align-center\">Для власного споживання</h3>\n                <div class=\"form-wrap pt-4\" mbr-form=\"\">\n<!--Formbuilder Form-->\n<form action=\"oleh.chernyuk@gmail.com\" name=\"Buyers Request\" method=\"POST\" class=\"mbr-form form-with-styler\">\n<div class=\"row\">\n<div hidden=\"hidden\" data-form-alert=\"\" class=\"alert alert-success col-12\">Дякуємо за ваше замовлення! Ми зв’яжемось із вами найближим часом!</div>\n<div hidden=\"hidden\" data-form-alert-danger=\"\" class=\"alert alert-danger col-12\">\n</div>\n</div>\n<div class=\"dragArea row\">\n<div class=\"col-lg-12 col-md-12 col-sm-12 mb-3 form-group\" data-for=\"Full_name\">\n<input type=\"text\" name=\"Full_name\" placeholder=\"Ваше ФІО*\" data-form-field=\"Full_name\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control input\" value=\"\">\n</div>\n<div class=\"col-lg-6 mb-3 form-group\" data-for=\"email\">\n<input type=\"email\" name=\"email\" placeholder=\"Email\" data-form-field=\"Email\" mbr-theme-style=\"display-7\" class=\"form-control input\" value=\"\">\n</div>\n<div data-for=\"Mobile_Number\" class=\"col-lg-6 mb-3 form-group\">\n<input type=\"text\" name=\"Mobile_Number\" placeholder=\"Ваш номер телефону*\" data-form-field=\"Mobile_Number\" mbr-theme-style=\"display-7\" class=\"form-control input\" required=\"required\" value=\"\">\n</div>\n<div class=\"col-lg-6 col-md-12 col-sm-12 form-group\">\n<div class=\"form-control-label\">\n<label mbr-text=\"\" mbr-theme-style=\"display-7\" for=\" Редис із фіолетовою ніжкою\" class=\"mbr-fonts-style\">Оберіть, що вас цікавить:</label>\n</div>\n<div data-for=\"Люцерна\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\"Люцерна\" data-form-field=\"Люцерна\" mbr-theme-style=\"display-7\" class=\"form-check-input\">\n<label mbr-text=\"\" for=\"Люцерна\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Люцерна</label>\n</div>\n<div data-for=\"Амарант\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\"Амарант\" data-form-field=\"Амарант\" mbr-theme-style=\"display-7\" class=\"form-check-input\">\n<label mbr-text=\"\" for=\"Амарант\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Амарант</label>\n</div>\n<div data-for=\"Кінза\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\"Кінза\" data-form-field=\"Кінза\" mbr-theme-style=\"display-7\" class=\"form-check-input\">\n<label mbr-text=\"\" for=\"Кінза\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Кінза</label>\n</div>\n<div data-for=\"Льон\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\"Льон\" data-form-field=\"Льон\" mbr-theme-style=\"display-7\" class=\"form-check-input\">\n<label mbr-text=\"\" for=\"Льон\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Льон</label>\n</div>\n<div data-for=\"Цибуля\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\"Цибуля\" data-form-field=\"Цибуля\" mbr-theme-style=\"display-7\" class=\"form-check-input\">\n<label mbr-text=\"\" for=\"Цибуля\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Цибуля</label>\n</div>\n<div data-for=\"Горох\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\"Горох\" data-form-field=\"Горох\" mbr-theme-style=\"display-7\" class=\"form-check-input\">\n<label mbr-text=\"\" for=\"Горох\" mbr-theme-style=\"display-7\" class=\"form-check-label\">Горох</label>\n</div>\n<div data-for=\" Редис Зелений\" class=\"form-check\">\n<input type=\"checkbox\" value=\"Yes\" name=\" Редис Зелений\" d